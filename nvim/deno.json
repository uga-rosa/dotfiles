{
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false
  },
  "lock": false,
  "tasks": {
    "cache:plugins": "deno task cache:lazy; deno task cache:dev",
    "cache:lazy": "deno cache --no-check $HOME/.local/share/nvim/lazy/**/denops/**/*.ts",
    "cache:dev": "deno cache --no-check $HOME/plugin/**/denops/**/*.ts"
  }
}
