snippet func
	function $1($2)
		$0
	end
snippet rt
	return $0
snippet for i in []
	for ${1:i} in ${2:[]}
		$0
	end
snippet for i = 1:10
	for ${1:i} = ${2:1}:${3:10}
		$0
	end
snippet if .. end
	if $1
		$0
	end
snippet elseif
	elseif $1
		$0
snippet else
	else
		$0
snippet while
	while $1
		$0
	end
snippet open() do f .. end
	open($1) do ${2:f}
		$0
	end
