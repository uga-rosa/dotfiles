{
  "import": {
    "prefix": [
      "import"
    ],
    "body": [
      "import ${1:module}${2: except ${3:symbol}}"
    ]
  },
  "from import": {
    "prefix": [
      "from import"
    ],
    "body": [
      "from ${1:module}${2: as ${3:name}} import ${4:symbol}"
    ]
  },
  "if": {
    "prefix": [
      "if"
    ],
    "body": [
      "if $1:",
      "  $2"
    ]
  },
  "if else": {
    "prefix": [
      "if else"
    ],
    "body": [
      "if $1:",
      "  $2",
      "else:",
      "  $3"
    ]
  },
  "elif": {
    "prefix": [
      "elif"
    ],
    "body": [
      "elif $1:",
      "  $2"
    ]
  },
  "else": {
    "prefix": [
      "else"
    ],
    "body": [
      "else:",
      "  $1"
    ]
  },
  "case": {
    "prefix": [
      "case"
    ],
    "body": [
      "case $1",
      "of $2:",
      "  $3"
    ]
  },
  "of": {
    "prefix": [
      "of"
    ],
    "body": [
      "of $1:",
      "  $2"
    ]
  },
  "while": {
    "prefix": [
      "while"
    ],
    "body": [
      "while $1:",
      "  $2"
    ]
  },
  "for": {
    "prefix": [
      "for"
    ],
    "body": [
      "for ${1:i} in $2:",
      "  $3"
    ]
  },
  "block": {
    "prefix": [
      "block"
    ],
    "body": [
      "block$1:",
      "  $2"
    ]
  },
  "proc": {
    "prefix": [
      "proc"
    ],
    "body": [
      "proc $1($2: $3): $4 =",
      "  $5"
    ]
  },
  "func": {
    "prefix": [
      "func"
    ],
    "body": [
      "func $1($2: $3): $4 =",
      "  $5"
    ]
  },
  "discard": {
    "prefix": [
      "discard"
    ],
    "body": [
      "discard "
    ]
  },
  "iter": {
    "prefix": [
      "iter"
    ],
    "body": [
      "iterator $1($2: $3): $4 =",
      "  $5"
    ]
  },
  "type": {
    "prefix": [
      "type"
    ],
    "body": [
      "type",
      "  "
    ]
  },
  "enum": {
    "prefix": [
      "enum"
    ],
    "body": [
      "enum",
      "  "
    ]
  },
  "object": {
    "prefix": [
      "object"
    ],
    "body": [
      "object",
      "  "
    ]
  },
  "rt": {
    "prefix": [
      "rt"
    ],
    "body": [
      "return "
    ]
  },
  "rs": {
    "prefix": [
      "rs"
    ],
    "body": [
      "result = "
    ]
  },
  "cligen": {
    "prefix": [
      "cligen"
    ],
    "body": [
      "import cligen",
      "dispatch($1)"
    ]
  },
  "try": {
    "prefix": [
      "try"
    ],
    "body": [
      "try:",
      "  $1",
      "except $2:",
      "  $3"
    ]
  },
  "except": {
    "prefix": [
      "except"
    ],
    "body": [
      "except $1:",
      "  $2"
    ]
  },
  "main": {
    "prefix": [
      "main"
    ],
    "body": [
      "when isMainModule:",
      "  "
    ]
  },
  "stdwrite": {
    "prefix": [
      "stdwrite"
    ],
    "body": [
      "stdout.write(\"$1\")"
    ]
  }
}