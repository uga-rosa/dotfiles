{
  "slide_init": {
    "prefix": [
      "slide_init"
    ],
    "body": [
      "% ${1:Title}",
      "% 中井 剛志",
      "% ${VIM:system(\"date '+%Y/%m/%d'\")}",
      "",
      ""
    ]
  },
  "pandoc_init": {
    "prefix": [
      "pandoc_init"
    ],
    "body": [
      "---",
      "title: ${1:Title}",
      "author: 分子集積化学研究室 計算グループ B4 中井 剛志",
      "date: ${VIM:system(\"date '+%Y/%m/%d'\")}",
      "",
      "---",
      "",
      ""
    ]
  },
  "left(": {
    "prefix": [
      "left("
    ],
    "body": [
      "\\\\left( $0 \\\\right)"
    ]
  },
  "left\\{": {
    "prefix": [
      "left\\{"
    ],
    "body": [
      "\\\\left\\\\{ $0 \\\\right\\\\}"
    ]
  },
  "left[": {
    "prefix": [
      "left["
    ],
    "body": [
      "\\\\left[ $0 \\\\right]"
    ]
  },
  "incremental": {
    "prefix": [
      "incremental"
    ],
    "body": [
      ":::incremental",
      "",
      "$0",
      "",
      ":::"
    ]
  },
  "sup": {
    "prefix": [
      "sup"
    ],
    "body": [
      "<sup>$1</sup>"
    ]
  },
  "sub": {
    "prefix": [
      "sub"
    ],
    "body": [
      "<sub>$1</sub>"
    ]
  },
  "right": {
    "prefix": [
      "right"
    ],
    "body": [
      "<div style='text-align': right>$1</div>"
    ]
  },
  "frac": {
    "prefix": [
      "frac"
    ],
    "body": [
      "${1|,\\\\displaystyle|}\\\\frac{$2}{$3}"
    ]
  },
  "formula_multi": {
    "prefix": [
      "formula_multi"
    ],
    "body": [
      "$$",
      "\\\\begin{${1|aligned,multiline|}}",
      "$0",
      "\\\\end{$1}",
      "$$"
    ]
  },
  "formula": {
    "prefix": [
      "formula"
    ],
    "body": [
      "$$",
      "$0",
      "$$"
    ]
  },
  "matrix": {
    "prefix": [
      "matrix"
    ],
    "body": [
      "\\\\begin{${1|p,v|}matrix}",
      "$0",
      "\\\\end{$1matrix}"
    ]
  },
  "sum": {
    "prefix": [
      "sum"
    ],
    "body": [
      "\\\\sum_{${1:i=1}}^{${2:n}}"
    ]
  },
  "prod": {
    "prefix": [
      "prod"
    ],
    "body": [
      "\\\\prod_{${1:i=1}}^{${2:n}}"
    ]
  },
  "int": {
    "prefix": [
      "int"
    ],
    "body": [
      "\\\\int_{${1:0}}^{${2:\\\\infty}}"
    ]
  },
  "tag": {
    "prefix": [
      "tag"
    ],
    "body": [
      "\\\\tag{$1}"
    ]
  },
  "sqrt": {
    "prefix": [
      "sqrt"
    ],
    "body": [
      "\\\\sqrt{$1}"
    ]
  },
  "arrow": {
    "prefix": [
      "arrow"
    ],
    "body": [
      "${1|\\\\right,\\\\Right,\\\\left,\\\\Left|}arrow"
    ]
  },
  "infinity": {
    "prefix": [
      "infinity"
    ],
    "body": [
      "\\\\infty"
    ]
  },
  "partial": {
    "prefix": [
      "partial"
    ],
    "body": [
      "\\\\partial"
    ]
  },
  "vector": {
    "prefix": [
      "vector"
    ],
    "body": [
      "${1|\\\\bm,\\\\overrightarrow|}{$2}"
    ]
  },
  "overline": {
    "prefix": [
      "overline"
    ],
    "body": [
      "\\\\overline$1"
    ]
  },
  "slide_title": {
    "prefix": [
      "slide_title"
    ],
    "body": [
      "<!-- _class: lead invert -->",
      "<!-- _paginate: false -->"
    ]
  },
  "slide_section": {
    "prefix": [
      "slide_section"
    ],
    "body": [
      "<!-- _class: gaia lead -->",
      "<!-- _paginate: false -->"
    ]
  },
  "details": {
    "prefix": [
      "details"
    ],
    "body": [
      "<details><summary>${1:menu}</summary><div>",
      "",
      "$2",
      "",
      "</div></details>",
      ""
    ]
  },
  "bold": {
    "prefix": [
      "bold"
    ],
    "body": [
      "**$1**"
    ]
  },
  "italic": {
    "prefix": [
      "italic"
    ],
    "body": [
      "*$1*"
    ]
  }
}